{
  "name": "comics",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": "6.11.3"
  },
  "description": "A Comic app with React & ExpressJs",
  "author": "Nathan Quach",
  "scripts": {
    "migrate:make": "./node_modules/knex/bin/cli.js migrate:make",
    "migrate:latest": "./node_modules/knex/bin/cli.js migrate:latest",
    "migrate:rollback": "./node_modules/knex/bin/cli.js migrate:rollback",
    "build": "rm -rf ./build && ./node_modules/.bin/webpack --config webpack.config.js --progress --colors",
    "start": "npm run build && node build/server.js",
    "dev": "rm -rf ./build && ./node_modules/.bin/webpack --config webpack.dev.config.js --progress --colors"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "bootstrap": "^4.0.0-alpha.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "immutable": "^3.8.1",
    "jade": "^1.11.0",
    "knex": "^0.12.9",
    "lodash": "4.17.4",
    "mysql": "^2.13.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.2.2",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "start-server-webpack-plugin": "^2.2.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.5.1",
    "transform-loader": "^0.2.4",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.5.4"
  }
}
